<h2>Edit <%= @address.nickname %></h2>

<% @address.errors.full_messages.each do |msg| %>
  <%= msg %>
<% end %>

<%= form_for @address, url: address_path do |f| %>

  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit "Edit Address"%>

<% end %>
