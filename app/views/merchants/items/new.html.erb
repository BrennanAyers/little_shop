<%= form_for [@merchant, @item], :url => '/dashboard/items' do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_area :description %>

<%= f.label :image %>
<%= f.text_field :image %>

<%= f.label :price %>
<%= f.text_field :price %>

<%= f.label :inventory %>
<%= f.text_field :inventory %>

<%= f.submit %>

<% end %>
